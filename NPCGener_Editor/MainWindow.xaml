        <TextBox x:Name="segment3range" HorizontalAlignment="Left" Margin="143,643,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203" IsEnabled="False"/>
        <Label Content="Segment3Range" HorizontalAlignment="Left" Margin="7,637,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
