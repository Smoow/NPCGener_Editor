        <Label Content="Segment3Y" HorizontalAlignment="Left" Margin="7,595,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="segment3wait" HorizontalAlignment="Left" Margin="143,621,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203" IsEnabled="False"/>
