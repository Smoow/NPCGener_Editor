        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NPCGener_Editor"
        mc:Ignorable="d"
        Title="NPCGener Editor - Smoow" Height="850" Width="1000"
        ResizeMode="CanMinimize" WindowStyle="SingleBorderWindow" WindowStartupLocation="CenterScreen">
    

    <Grid Margin="10,10,10,-6" Width="980">
        <Grid.RowDefinitions>
            <RowDefinition Height="401*"/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="formation" HorizontalAlignment="Left" Margin="143,201,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label Content="Formation&#xA;" HorizontalAlignment="Left" Margin="7,197,0,0" VerticalAlignment="Top" Width="131" Height="24" FontWeight="Bold"/>
        <TextBox x:Name="startX" HorizontalAlignment="Left" Margin="143,225,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label Content="StartX" HorizontalAlignment="Left" Margin="7,221,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="startY" HorizontalAlignment="Left" Margin="143,248,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label Content="StartY&#xA;" HorizontalAlignment="Left" Margin="7,244,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="startRange" HorizontalAlignment="Left" Margin="143,270,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label Content="StartRange" HorizontalAlignment="Left" Margin="7,267,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="startWait" HorizontalAlignment="Left" Margin="143,292,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
