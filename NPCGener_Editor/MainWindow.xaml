        <CheckBox x:Name="segment1ActionCheckBox" Content="Segment1Action" Click="segment1ActionFunc" HorizontalAlignment="Left" Margin="380,230,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="segment2ActionCheckBox" Content="Segment2Action" Click="segment2ActionFunc" HorizontalAlignment="Left" Margin="380,249,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="segment3ActionCheckBox" Content="Segment3Action" Click="segment3ActionFunc" HorizontalAlignment="Left" Margin="380,267,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="startActionText" HorizontalAlignment="Left" Margin="143,664,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="323" IsEnabled="False"/>
