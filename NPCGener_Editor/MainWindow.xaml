        <Label Content="Segment2X" HorizontalAlignment="Left" Margin="7,486,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="segment2y" HorizontalAlignment="Left" Margin="143,513,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203" IsEnabled="False"/>
