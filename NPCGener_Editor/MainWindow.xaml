        <TextBox x:Name="segment1ActionText" HorizontalAlignment="Left" Margin="143,687,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="323" IsEnabled="False"/>
        <Label Content="Segment1Action" HorizontalAlignment="Left" Margin="7,681,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="segment2ActionText" HorizontalAlignment="Left" Margin="143,708,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="323" IsEnabled="False"/>
        <Label Content="Segment2Action" HorizontalAlignment="Left" Margin="7,701,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="segment3ActionText" HorizontalAlignment="Left" Margin="143,730,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="323" IsEnabled="False"/>
        <Label Content="Segment3Action" HorizontalAlignment="Left" Margin="7,723,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
