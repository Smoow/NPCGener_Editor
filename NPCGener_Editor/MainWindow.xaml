        <Label Content="DestY" HorizontalAlignment="Left" Margin="7,331,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="destRange" HorizontalAlignment="Left" Margin="143,358,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label Content="DestRange" HorizontalAlignment="Left" Margin="7,352,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
        <TextBox x:Name="destWait" HorizontalAlignment="Left" Margin="143,380,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label Content="DestWait" HorizontalAlignment="Left" Margin="7,374,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>



        <TextBox x:Name="desc" HorizontalAlignment="Left" Margin="143,3,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="334"/>
        <Button x:Name="btn1" Click="SendDataToNPCGenerGeneric" Content="Adicionar" HorizontalAlignment="Left" Margin="367,466,0,0" VerticalAlignment="Top" Height="29" Width="99"/>
        <Label Content="Descrição" HorizontalAlignment="Left" Margin="7,-1,0,0" VerticalAlignment="Top" Width="131" Height="26" FontWeight="Bold"/>
