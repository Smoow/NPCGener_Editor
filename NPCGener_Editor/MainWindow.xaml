        <TextBox x:Name="segment1range" HorizontalAlignment="Left" Margin="143,468,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203" IsEnabled="False"/>
        <Label Content="Segment1Range" HorizontalAlignment="Left" Margin="7,462,0,0" VerticalAlignment="Top" Width="131" Height="25" FontWeight="Bold"/>
